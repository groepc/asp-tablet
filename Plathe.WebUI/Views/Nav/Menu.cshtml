@model IEnumerable<string>

@Html.ActionLink("Alle films", "List", "Genre", null,
    new { @class = "nav nav-pills nav-stacked" })

@foreach (var link in Model)
{
    @Html.RouteLink(link, new object
    {
        name = "GenreID",
        action = "List",
        category = link
                
    }, new
    {
        @class = "btn btn-block btn-default btn-lg"
            + (link == ViewBag.SelectedCategory ? "btn-primary" : "")
    })
}
